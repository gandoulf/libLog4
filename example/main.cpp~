#include "log4.hpp"

int main(int argc, char **argv)
{
	Log4 log("test");
	log.initLogger();
	log.addLogger(&(std::string("normal")));
	log.initXML(NULL);
	log.useLogger(&(std::string("normal")), &(std::string("hello world")), INFO);
	return (0);
}
